<%= form_for @document, html: { multipart: true } do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %></br>
      <%= f.label :discription %>
      <%= f.text_field :discription %></br>
      <%= f.label :attachment %>
      <%= f.file_field :attachment %></br>
      <%= f.submit "Create"%>
 <% end %>
