


<h1> Edit File file </h1>



<%= form_for :post, url: post_path(@post), method: :patch do |f| %> <br>

	<p>
		<%= f.label :name %> <br>
		<%= f.text_field(:title, {:class => 'form-control'}) %>
	</p>

	<p>
		<%= f.label :body %> <br>
		<%= f.text_area(:body, {:class => 'form-control'}) %>
	</p>

	<p>
		<%= f.submit ({:class => 'btn btn-primary'})%> <br>
	</p>

<% end %>	