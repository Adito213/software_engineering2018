<%= form_with scope: :calculation, url: calculations_path, local: true do |form| %>
  <p>
    <%= form.label :a %><br>
    <%= number_field_tag(:a) %>
  </p>
 
  <p>
    <%= form.label :b %><br>
    <%= number_field_tag(:b) %>
  </p>
  
  <p>
    <%= form.label :c %><br>
    <%= number_field_tag(:c) %>
  </p>
 
  <p>
    <%= form.submit "Solve equation." %>
  </p>
<% end %>
